{
  "extends": ["tslint:latest", "tslint-eslint-rules", "tslint-react", "tslint-config-prettier"],
  "jsRules": {
    "class-name": false,
    "comment-format": [false, "check-space"],
    "eofline": false,
    "forin": false,
    "indent": [false, "spaces", 2],
    "label-position": false,
    "max-line-length": [false, 180],
    "member-access": false,
    "member-ordering": [false, "static-before-instance", "variables-before-functions"],
    "no-arg": false,
    "no-bitwise": false,
    "no-console": [false, "log", "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": false,
    "no-debugger": false,
    "no-duplicate-variable": false,
    "no-empty": false,
    "no-eval": false,
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": false,
    "no-unused-expression": [false, "allow-fast-null-checks"],
    "no-var-keyword": false,
    "no-implicit-dependencies": [false, "dev"],
    "object-literal-sort-keys": false,
    "one-line": [false, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
    "quotemark": [false, "single", "jsx-double", "avoid-escape"],
    "radix": false,
    "semicolon": [false, "always", "ignore-bound-class-methods"],
    "triple-equals": [false, "allow-null-check"],
    "variable-name": false,
    "whitespace": [false, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"],
    "prefer-const": false,
    "arrow-parens": [false, "ban-single-arg-parens"],
    "arrow-return-shorthand": [false],
    "import-spacing": false,
    "no-consecutive-blank-lines": [false],
    "object-literal-shorthand": false,
    "space-before-function-paren": [false, { "named": "never", "asyncArrow": "always" }],
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "ordered-imports": [false],
    "curly": [false, "ignore-same-line"],
    "switch-default": false,
    "cyclomatic-complexity": [false, 40],
    "no-invalid-this": [false],
    "no-magic-numbers": false,

    "ter-arrow-body-style": [false],
    "ter-no-irregular-whitespace": [false],
    "ter-no-sparse-arrays": [false],
    "ter-func-call-spacing": [false, "never"],
    "no-multi-spaces": [false],
    "brace-style": [
      false,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "object-curly-spacing": [false, "always"],
    "space-in-parens": [false, "never"]
  },
  "rules": {
    "class-name": false,
    "comment-format": [false, "check-space"],
    "eofline": false,
    "forin": false,
    "indent": [false, "spaces", 2],
    "label-position": false,
    "max-line-length": [false, 180],
    "member-access": false,
    "member-ordering": [true, "static-before-instance", "variables-before-functions"],
    "no-arg": false,
    "no-bitwise": false,
    "no-console": [false, "log", "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": false,
    "no-debugger": false,
    "no-duplicate-variable": false,
    "no-eval": false,
    "no-inferrable-types": [false],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-switch-case-fall-through": false,
    "no-trailing-whitespace": false,
    "no-unused-expression": [false, "allow-fast-null-checks"],
    "no-var-keyword": false,
    "no-implicit-dependencies": [false, "dev"],
    "object-literal-sort-keys": false,
    "one-line": [false, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
    "quotemark": [false, "single", "jsx-double", "avoid-escape"],
    "radix": false,
    "semicolon": [false, "always", "ignore-bound-class-methods"],
    "triple-equals": [false, "allow-null-check"],
    "typedef-whitespace": [
      false,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": false,
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast",
      "check-preblock"
    ],
    "prefer-const": false,
    "arrow-parens": [false, "ban-single-arg-parens"],
    "arrow-return-shorthand": [false],
    "import-spacing": false,
    "no-consecutive-blank-lines": [false],
    "object-literal-shorthand": false,
    "space-before-function-paren": [false, { "named": "never", "asyncArrow": "always" }],
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "ordered-imports": [false],
    "curly": [false, "ignore-same-line"],
    "switch-default": false,
    "cyclomatic-complexity": [false, 40],
    "no-invalid-this": [false],
    "no-magic-numbers": false,
    "ban-types": [false, ["Object", "Use {} instead."]],
    "no-submodule-imports": [false, "app", "uuid/v4", "@fortawesome", "lodash", "react-toastify"],

    "ter-arrow-body-style": [false],
    "ter-no-irregular-whitespace": [false],
    "ter-no-sparse-arrays": [false],
    "ter-func-call-spacing": [false, "never"],
    "no-multi-spaces": [false],
    "array-bracket-spacing": [false],
    "brace-style": [
      false,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "object-curly-spacing": [false, "always"],
    "space-in-parens": [false, "never"],

    "jsx-boolean-value": [false, "never"],
    "jsx-no-multiline-js": false,
    "jsx-alignment": false
  },
  "rulesDirectory": []
}
